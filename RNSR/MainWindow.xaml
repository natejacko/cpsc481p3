            </Grid>
            <Grid x:Name="Footer" Height="70" VerticalAlignment="Bottom" Background="#FF7A7979">
                <Grid x:Name="MapButton" Margin="0,0,824,0" Background="#FF7A7979" MouseDown="MapButton_MouseDown">
                    <TextBlock HorizontalAlignment="Left" Height="30.5" Margin="73,19,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="54" FontWeight="Bold" FontSize="24" Foreground="White"><Run Text="Map"/><LineBreak/><Run/></TextBlock>
                </Grid>
                <Grid x:Name="ViewOrderButton" Margin="206,0,618,0" Background="#FF7A7979" MouseDown="ViewOrderButton_MouseDown">
                    <TextBlock HorizontalAlignment="Left" Height="34" Margin="34,19,0,0" TextWrapping="Wrap" Text="View Order" VerticalAlignment="Top" Width="132" FontWeight="Bold" FontSize="24" Foreground="White"/>
                </Grid>
                <Grid x:Name="MenuButton" Margin="412,0" Background="#FF7A7979" MouseDown="MenuButton_MouseDown">
                    <TextBlock HorizontalAlignment="Left" Height="31" Margin="67,19,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="67" FontWeight="Bold" FontSize="24" Foreground="White"><Run Text="Menu"/><LineBreak/><Run/></TextBlock>
                </Grid>
