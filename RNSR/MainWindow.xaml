                <Button x:Name="View2" Content="Patio" HorizontalAlignment="Left" Height="126.535" Margin="10,347.182,0,0" VerticalAlignment="Top" Width="180" RenderTransformOrigin="0.502,-0.846" Background="Black" Foreground="White" FontSize="48" Click="View2_Click"/>
            </Grid>
