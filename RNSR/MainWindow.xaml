                <Button x:Name="Table10" Content="Table#10" HorizontalAlignment="Left" Height="115.991" Margin="539.533,428.023,0,0" VerticalAlignment="Top" Width="151.14" FontSize="36" Background="#FF070738" Foreground="White" Click="Table10_Click"/>
            </Grid>
            <Grid x:Name="Viewer1" Margin="0,0,200,0" Background="#FF910000">
                <Button x:Name="Table1" Content="Table #1&#xA;" HorizontalAlignment="Left" Height="154.654" Margin="56.238,64.234,0,0" VerticalAlignment="Top" Width="230.225" Background="#FF070738" FontSize="36" Foreground="WhiteSmoke" Click="Table1_Click"/>
                <Button x:Name="Table2" Content="Table #2&#xA;" HorizontalAlignment="Left" Height="154.654" Margin="514.929,64.234,0,0" VerticalAlignment="Top" Width="226.71" Background="#FF070738" FontSize="36" Foreground="WhiteSmoke" Click="Table2_Click"/>
                <Button x:Name="Table4" Content="Table #4&#xA;" HorizontalAlignment="Left" Height="147.624" Margin="514.929,391.118,0,0" VerticalAlignment="Top" Width="226.71" Background="#FF070738" FontSize="36" Foreground="WhiteSmoke" Click="Table4_Click"/>
                <Button x:Name="Table3" Content="Table #3&#xA;" HorizontalAlignment="Left" Height="147.624" Margin="56.238,391.118,0,0" VerticalAlignment="Top" Width="230.225" Background="#FF070738" FontSize="36" Foreground="WhiteSmoke" Click="Table3_Click"/>
            </Grid>
            <Grid x:Name="ViewSelector1" Margin="824,0,0,0" Background="#FF303030">
