<UserControl x:Class="RNSR.ItemListControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:RNSR"
             mc:Ignorable="d" 
             d:DesignHeight="648" d:DesignWidth="300">
    <Grid>
        <Grid x:Name="ItemList" Margin="0" Background="#FF8D8D8D">
            <ScrollViewer x:Name="Scroller" Margin="0">
                <StackPanel x:Name="Items"/>
            </ScrollViewer>
        </Grid>

    </Grid>
</UserControl>
